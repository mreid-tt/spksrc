# udev rules for USB audio devices
# Allow access to USB audio devices for all users
SUBSYSTEM=="sound", KERNEL=="controlC*", MODE="0666"
SUBSYSTEM=="sound", KERNEL=="pcmC*D*c", MODE="0666"
SUBSYSTEM=="sound", KERNEL=="pcmC*D*p", MODE="0666"
SUBSYSTEM=="sound", KERNEL=="hwC*D*", MODE="0666"
SUBSYSTEM=="sound", KERNEL=="midiC*D*", MODE="0666"
